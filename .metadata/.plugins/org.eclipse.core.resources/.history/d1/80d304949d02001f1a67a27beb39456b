package com.ezen.test.domain;

public class PagingVO {

	// select * from board limit 0, 10;		0, 10의 데이터를 가져올 매개변수 생성
	// 페이징 > pageNo / qty
	// 검색 > type / keyword
	
	private int pageNo;
	private int qty;	// 한 화면에 보여줄 게시글 수(10개)
	
	private String type;		// 검색유형(제목, 작성자, 내용)
	private String keyword;		// 검색어
	
	public PagingVO() {
		this.pageNo = 1;	// 기본값
		this.qty = 10;		// 기본값
	}
	
	
}
